@{
    Layout = null;
}
<!doctype html>
<html lang="ru">
<head>
    <meta charset="utf-8" />
    <title>Меню</title>
    <link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon">
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Reem+Kufi&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat&display=swap" rel="stylesheet">
    <style>
        .navigation {
            position: absolute;
            width: 205px;
            height: 29px;
            left: 21px;
            top: 10px;
            font-family: Montserrat;
            font-style: normal;
            font-weight: 600;
            font-size: 24px;
            line-height: 29px;
            color: #000000;
        }

        .menu {
            position: absolute;
            width: 205px;
            height: 29px;
            left: 125px;
            top: 10px;
            font-family: Montserrat;
            font-style: normal;
            font-weight: 600;
            font-size: 24px;
            line-height: 29px;
            color: #000000;
        }

        .ellipse1 {
            position: absolute;
            left: 0%;
            bottom: 0%;
        }

        .ellipse2 {
            position: absolute;
            right: 0%;
            top: 0%;
        }

        .flex-container {
            display: flex;
            flex-wrap: wrap;
        }

        .item {
            @*            border: 3px solid #FF6D03;
            border-radius: 14px;*@
            margin-top: 5%;
            text-align: center;
            flex: 0 0 33.333333%;
        }

        .item-text {
            width: 48%;
            margin-left: auto;
            margin-right: auto;
            font-family: Montserrat;
            font-style: normal;
            font-weight: 500;
            font-size: 200%;
            line-height: 32px;
            text-align: center;
            color: #000000;
        }
    </style>
</head>
<body>
    <img src="images/ellipse1.png" class="ellipse1">
    <img src="images/ellipse2.png" class="ellipse2">
    <a href="index.html"><div class="navigation">Главная</div></a>
    <a href="menu.html"><div class="menu">>Меню</div></a>
    <div class="flex-container">
        @foreach (var m in ViewBag.mealtimes)
        {
            <div class="item">
                <a href="breakfast.html">
                    <img src="images/2 @m.mealtimeId .png">
                    <div class="item-text">@m.mealtime_name</div>
                </a>
            </div>
        }
</div>
</body>
</html>
